Bulls =  H;
Tickers = C;

if ( GroupID() == 253 AND StrLeft( Name(), 2 ) == "~i" )
{
    sFBu = Null;
    sFT   = Null;
    iFBu = H;
    iFT   = C;
}
else
    if ( GroupID() == 253 AND StrLeft( Name(), 2 ) == "~s" )
    {
        sFBu = H;
        sFT = C;
