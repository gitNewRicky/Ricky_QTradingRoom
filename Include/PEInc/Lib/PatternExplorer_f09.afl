        sFT   = Foreign( "~s" + WriteVal( SectorID(), 1.0 ) + " - " + SectorID( 1 ), "C" );
    }

sFBu_p = 100 * sFBu / sFT;

sFBe_p = 100 - sFBu_p;
iFBu_p = 100 * iFBu / iFT;
iFBe_p = 100 - iFBu_P;
TickersDB = Foreign( "~Tickers_in_DB", "C" );
missing_quotes_filter = Highest( TickersDB ) - TickersDB > 10;

if ( GroupID() == 253 )
{
    if ( StrFind( Name(), "~s" ) )
